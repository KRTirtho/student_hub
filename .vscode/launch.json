{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "PB Launch",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "args": [
        "serve",
        "--dir",
        "${workspaceFolder}/pb/pb_data"
      ],
      "program": "${workspaceFolder}/pb/lib/src",
      "console": "integratedTerminal"
    },
    {
      "name": "eusc-freaks",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "eusc-freaks (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "eusc-freaks (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}